@model List<Slider>
@{
    ViewData["Title"] = "Index";
}


    <table class="table">
  <thead>
    <tr>
      <th scope="col">Image</th>
            <th scope="col">FullName</th>
            <th>Description</th>
            <th>Position</th>

    </tr>
  </thead>
  <tbody>
      @foreach (var item in Model)
{
    <tr>
              <td>@item.Images</td>

      <td>@item.FullName</td>
      <td>@item.Description</td>
            <td>@item.Lorem</td>
                        <td><a asp-action="Update" asp-controller="Slider" asp-area="Manage" asp-route-Id="@item.Id">Update</a></td>
                                                <td><a asp-action="Delete" asp-controller="Slider" asp-area="Manage" asp-route-Id="@item.Id">Delete</a></td>



    </tr>
}
 
  </tbody>
</table>
    
    
}
